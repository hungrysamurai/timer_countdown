{
  "name": "timer_countdown",
  "version": "1.0.0",
  "description": "Timer & Countdown",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "parcel index.html --dist-dir ./dev",
    "build": "node ../../_scripts/cleanDir.js ./build && parcel build index.html --dist-dir ./build && node ../../_scripts/cleanDir.js .parcel-cache",
    "deploy": "node ../../_scripts/cleanDir.js ../../server/dist/public/projects/timer_countdown && parcel build index.html --dist-dir ../../server/dist/public/projects/timer_countdown && node ../../_scripts/cleanDir.js .parcel-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hungrysamurai/timer_countdown.git"
  },
  "keywords": [
    "timer",
    "countdown"
  ],
  "author": "hungrysamurai",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hungrysamurai/timer_countdown/issues"
  },
  "homepage": "https://github.com/hungrysamurai/timer_countdown#readme",
  "devDependencies": {
    "parcel": "^2.12.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.4"
  },
  "targets": {
    "default": {
      "publicUrl": "./"
    }
  }
}
